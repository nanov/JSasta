// jastat-summary: If-else-if chain
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

let x: i32 = 15;

let branch: i32 = 0;
if (x < 10) {
    // io.println("x is less than 10");
    branch = 1;
} else if (x < 20) {
    // io.println("x is between 10 and 19");
    branch = 2;
} else {
    // io.println("x is 20 or greater");
    branch = 3;
}

t.assert.equals(2, branch);
