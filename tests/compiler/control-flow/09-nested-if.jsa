// jastat-summary: Nested if statements
// jastat: mode=run, capture=stdout

import io from @io;

let x: i32 = 10;
let y: i32 = 20;

if (x > 5) {
    if (y > 15) {
        io.println("Both conditions true");
    } else {
        io.println("Only first condition true");
    }
} else {
    io.println("First condition false");
}
