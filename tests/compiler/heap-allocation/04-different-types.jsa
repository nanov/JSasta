// jastat-summary: Heap allocation of different integer types
// jastat: mode=run, capture=stdout

import io from @io;

let arr1 = new i32[2];
let arr2 = new i64[2];

arr1[0] = 10;
arr1[1] = 20;
arr2[0] = 100;
arr2[1] = 200;

io.println("i32: {}, {}", arr1[0], arr1[1]);
io.println("i64: {}, {}", arr2[0], arr2[1]);

delete arr1;
delete arr2;
