// jastat-summary: Heap allocation of different integer types
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

let arr1 = new i32[2];
let arr2 = new i64[2];

arr1[0] = 10;
arr1[1] = 20;
arr2[0] = 100;
arr2[1] = 200;

// io.println("i32: {}, {}", arr1[0], arr1[1]);
// io.println("i64: {}, {}", arr2[0], arr2[1]);

t.assert.equals(10, arr1[0]);
t.assert.equals(20, arr1[1]);
t.assert.equals(100, arr2[0]);
t.assert.equals(200, arr2[1]);

delete arr1;
delete arr2;
