// jastat-summary: Basic heap allocation and deallocation with new/delete
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

let arr = new i32[3];
arr[0] = 10;
arr[1] = 20;
arr[2] = 30;

// io.println("arr[0] = {}", arr[0]);
// io.println("arr[1] = {}", arr[1]);
// io.println("arr[2] = {}", arr[2]);

t.assert.equals(10, arr[0]);
t.assert.equals(20, arr[1]);
t.assert.equals(30, arr[2]);

delete arr;
