// jastat-summary: Unary minus operator
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

let a: i32 = 42;
let b: i32 = -a;

// io.println("a = {}", a);
// io.println("-a = {}", b);
// io.println("-(-a) = {}", -b);
t.assert.equals(42, a);
t.assert.equals(-42, b);
t.assert.equals(42, -b);
