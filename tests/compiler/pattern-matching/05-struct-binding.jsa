// Test pattern matching with struct binding
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

enum Message {
    quit;
    move(x: i32, y: i32);
}

let msg = Message.move{x: 10, y: 20};

let branch1: i32 = 0;
if (msg is Message.move(let m)) {
    // io.println("Move to x={}, y={}", m.x, m.y);
    t.assert.equals(10, m.x);
    t.assert.equals(20, m.y);
    branch1 = 1;
}

t.assert.equals(1, branch1);

let branch2: i32 = 0;
if (msg is Message.quit) {
    // io.println("Quit");
    branch2 = 1;
} else {
    // io.println("Not quit");
    branch2 = 2;
}

t.assert.equals(2, branch2);
