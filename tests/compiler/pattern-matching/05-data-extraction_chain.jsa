// Test pattern matching with data extraction
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

enum Message {
    quit;
    move(x: i32, y: i32);
}

let msg = Message.move{x: 10, y: 20};

let branch: i32 = 0;
if (msg is Message.move(let x, let y) && x == 10) {
    // io.println("Move to x={}, y={}", x, y);
    t.assert.equals(10, x);
    t.assert.equals(20, y);
    branch = 1;
}

t.assert.equals(1, branch);
