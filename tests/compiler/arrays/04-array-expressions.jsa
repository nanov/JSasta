// jastat-summary: Using array elements in expressions
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

let arr: i32[3] = {10, 20, 30};

let sum: i32 = arr[0] + arr[1] + arr[2];
let product: i32 = arr[0] * arr[1];

t.assert.equals(60, sum);
t.assert.equals(200, product);
