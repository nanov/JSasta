// jastat-summary: Array index access with different types
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

let int_arr: i32[3] = {1, 2, 3};
let bool_arr: bool[2] = {true, false};

t.assert.equals(1, int_arr[0]);
t.assert.equals(3, int_arr[2]);
t.assert.equals(true, bool_arr[0]);
t.assert.equals(false, bool_arr[1]);
