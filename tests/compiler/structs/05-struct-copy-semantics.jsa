// jastat-summary: Struct copy vs reference semantics
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

struct Value {
    n: i32;
}

var v1: Value;
v1.n = 10;
var v2 = v1;  // Copy

v2.n = 20;

// io.println("v1.n = {}", v1.n);
// io.println("v2.n = {}", v2.n);
t.assert.equals(10, v1.n);
t.assert.equals(20, v2.n);
