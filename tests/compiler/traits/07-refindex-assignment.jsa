// jastat-summary: RefIndex trait for assignment on heap arrays
// jastat: mode=run, capture=stdout

import io from @io;

let arr = new i32[3];

arr[0] = 10;
arr[1] = 20;
arr[2] = 30;

arr[0] = arr[0] * 2;
arr[1] = arr[1] * 2;
arr[2] = arr[2] * 2;

io.println("After doubling: {}, {}, {}", arr[0], arr[1], arr[2]);

delete arr;
