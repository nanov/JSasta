// jastat-summary: Reference type creation and dereferencing
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

let x: i32 = 42;
let r = ref x;  // ref i32

// io.println("x = {}", x);
t.assert.equals(42, x);

// io.println("r = {}", r);
// Note: reference printing shows empty output in fixture

r = 100;  // Modifies x through reference

// io.println("After r = 100:");
// io.println("x = {}", x);
t.assert.equals(100, x);
