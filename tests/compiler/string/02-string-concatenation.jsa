// jastat-summary: String concatenation with + operator
// jastat: mode=run, capture=assert

import test from "@test";
import io from "@io";

let s1: str = "Hello";
let s2: str = " ";
let s3: str = "World";

let result: str = s1 + s2 + s3;

io.println("{}", result);
test.assert.equals("Hello World", result);
