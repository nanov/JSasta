// jastat-summary: Early return from function
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

function abs(n: i32): i32 {
    if (n < 0) {
        return -n;
    }
    return n;
}

// io.println("abs(-5) = {}", abs(-5));
// io.println("abs(5) = {}", abs(5));

t.assert.equals(5, abs(-5));
t.assert.equals(5, abs(5));
