// jastat-summary: Basic enum with unit variants only
// jastat: mode=run, capture=stdout

import io from @io;

enum Status {
    Pending;
    Running;
    Complete;
}

let s1 = Status.Pending;
let s2 = Status.Running;
let s3 = Status.Complete;

// Test that variants are distinct
if (s1 == Status.Pending) {
    io.println("s1 is Pending");
}

if (s2 is Status.Running) {
    io.println("s2 is Running");
}

if (s3 is Status.Complete) {
    io.println("s3 is Complete");
}

if (!(s1 is Status.Running)) {
    io.println("s1 is not Running");
}
