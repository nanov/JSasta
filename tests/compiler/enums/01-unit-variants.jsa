// jastat-summary: Basic enum with unit variants only
// jastat: mode=run, capture=assert

import io from @io;
import t from @test;

enum Status {
    Pending;
    Running;
    Complete;
}

let s1 = Status.Pending;
let s2 = Status.Running;
let s3 = Status.Complete;

// Test that variants are distinct
t.assert.equals(Status.Pending, s1);
t.assert.equals(Status.Running, s2);
t.assert.equals(Status.Complete, s3);
t.assert.not_equals(Status.Running, s1);
