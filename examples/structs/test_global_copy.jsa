external printf(string, ...):void;

struct Point {
    x: i32;
    y: i32;
}

// Test global struct copy
const p1: Point;
p1.x = 10;
p1.y = 20;

var p2 = p1;  // Global copy

printf("Test global copy:\n");
printf("  p1.x=%d, p2.x=%d\n", p1.x, p2.x);

p2.x = 100;
printf("  After p2.x=100: p1.x=%d, p2.x=%d\n", p1.x, p2.x);

if (p1.x == 10 && p2.x == 100) {
    printf("  SUCCESS: p1 and p2 are independent copies!\n");
} else {
    printf("  FAILED: Expected p1.x=10, p2.x=100\n");
}
